type Event {
  id: ID!
  uid: ID!
  type: String!
  date: String!
}

type Query {
  events(id:ID!): [Event!]
  reportsEvents(mid:ID!): [Event!]
}

input NewEvent {
  user: String!
  type: String!
}

type Mutation {
  createEvent(input: NewEvent!): Event!
}

type Subscription {
  events(id:ID): Event!
}